{
    "name": "lightstep/tracer",
    "description": "LightStep instrumentation API",
    "license": "MIT",
    "require": {
        "ruafozy/mersenne-twister": "^1.3",
        "google/protobuf": ">=3.6.1",
        "ext-bcmath": "*",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "^2.8.5",
        "phpunit/phpunit": "~4.8.20"
    },
    "autoload": {
        "psr-4": {
            "LightStepBase\\": "lib/",
            "Lightstep\\": "generated/Lightstep/",
            "GPBMetadata\\": "generated/GPBMetadata/",
            "Google\\Api\\": "generated/Google/Api/",
            "Thrift\\": "thrift/Thrift/"
        },
        "files": [
            "lib/api.php",
            "lib/class_aliases.php",
            "lib/Client/Version.php",
            "thrift/CroutonThrift/ReportingService.php",
            "thrift/CroutonThrift/Types.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "": "test"
        }
    },
    "version": "1.1.2",
    "config": {
        "sort-packages": true
    }
}
